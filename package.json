{
  "name": "joviapp",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "migrate:gigs": "ts-node --project scripts/tsconfig.json scripts/migrateGigs.ts",
    "migrate:gigs:dry-run": "ts-node --project scripts/tsconfig.json scripts/migrateGigs.ts dry-run",
    "migrate:gigs:validate": "ts-node --project scripts/tsconfig.json scripts/migrateGigs.ts validate",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes",
    "seed:gig-sources": "ts-node --project scripts/tsconfig.json scripts/seedGigSources.ts",
    "seed:gig-sources:list": "ts-node --project scripts/tsconfig.json scripts/seedGigSources.ts list",
    "seed:gig-sources:clear": "ts-node --project scripts/tsconfig.json scripts/seedGigSources.ts clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/material-top-tabs": "^7.4.13",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.3.14",
    "@tanstack/react-query": "^5.64.0",
    "expo": "~54.0.32",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.8.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "^8.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "firebase-admin": "^13.0.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
